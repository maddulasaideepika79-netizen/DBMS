declare 
grade char(1); 
begin 
grade:='&grade'; 
case 
when grade='a' then 
dbms_output.put_line('Excellent'); 
when grade='b' then 
dbms_output.put_line('very good'); 
when grade='c' then 
dbms_output.put_line('good'); 
when grade='d' then 
dbms_output.put_line('fair'); 
when grade='f' then 
dbms_output.put_line('poor'); 
else 
dbms_output.put_line('No such grade'); 
end case; 
end; 
/ 
